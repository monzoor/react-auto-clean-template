      "@components/*": ["components/*", "components/index.js"],
      "@assets/*": ["assets/*", "assets/index.js"],
      "@actions/*": ["actions/*", "actions/index.js"],
      "@utils/*": ["utils/*", "utils/index.js"],
      "@validations/*": ["validations/*", "validations/index.js"],
      "@helpers/*": ["helpers/*", "helpers/index.js"],
      "@constants/*": ["constants/*", "constants/index.js"],
